<%- include("header") -%>

  <div class="box" id="heading">
    <h1 id="heading"><%= kindOfDay %></h1>
  </div>
  <div class="box">
    <% for (var i = 0; i < items.length; i++) {%>
    <div class="item">
      <input type="checkbox">
      <p><%= items[i] %></p>
    </div>
    <% } %>


    <form class="item" action="/" method="post">
      <input type="text" name="item" placeholder="Enter an item here" autocomplete="off">
      <button type="submit" name="submit">+</button>
    </form>
  </div>

<%- include("footer") -%>
